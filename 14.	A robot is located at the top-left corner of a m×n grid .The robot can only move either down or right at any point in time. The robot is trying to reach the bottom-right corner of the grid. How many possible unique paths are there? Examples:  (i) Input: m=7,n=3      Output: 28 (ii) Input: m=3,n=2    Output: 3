#include <stdio.h>

int uniquePaths(int m, int n) {
    int dp[m][n];
    
    for (int i = 0; i < m; i++) {
        dp[i][0] = 1;
    }
    
    for (int j = 0; j < n; j++) {
        dp[0][j] = 1;
    }
    
    for (int i = 1; i < m; i++) {
        for (int j = 1; j < n; j++) {
            dp[i][j] = dp[i - 1][j] + dp[i][j - 1];
        }
    }
    
    return dp[m - 1][n - 1];
}

int main() {
    int m1 = 7, n1 = 3;
    int m2 = 3, n2 = 2;
    
    printf("Unique paths for m=7, n=3: %d\n", uniquePaths(m1, n1));
    printf("Unique paths for m=3, n=2: %d\n", uniquePaths(m2, n2));
    
    return 0;
}
